<div class="invalid_notice"><%= notice %></div>

<div class="loginform">

<h1>Administrator Log In!</h1>

<%= form_tag adminsessions_path do %>
  <div class="field">
    <%= label_tag :email %> <br />
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %> <br />
    <%= password_field_tag :password %>
  </div>
  <div class="actions">
    <%= submit_tag "Log In" %>
  </div
<%end%>

</div>