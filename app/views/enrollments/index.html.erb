<div class="listing_enrolls container-fluid">
<h1>Listing Enrollments</h1>

<table>
  <thead>
    <tr>
      <th>Student ID</th>
      <th colspan="3">Course ID</th>
    </tr>
  </thead>

  <tbody>
    <% @enrollment.each do |enrollment| %>
      <tr>
        <td><%= enrollment.user_id %></td>
        <td><%= enrollment.course_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', adminhome_path %>

</div>
